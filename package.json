{
  "name": "@chatwoot/mobile-app",
  "version": "1.3.27",
  "private": true,
  "scripts": {
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "pod-install": "cd ios && bundle exec pod install",
    "clean:hard": "react-native-clean-project",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean",
    "android:release": "react-native run-android --variant=release",
    "android:publish-internal": "cd android && ./gradlew clean && fastlane android beta_internal",
    "android:publish-open": "cd android && ./gradlew clean && fastlane android beta_open",
    "ios:publish-internal": "cd ios && fastlane ios beta",
    "ios": "react-native run-ios",
    "ios:clean-derived-data": "rm -rf ~/Library/Developer/Xcode/DerivedData/*",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@chatwoot/react-native-widget": "0.0.6",
    "@eva-design/eva": "2.1.1",
    "@kesha-antonov/react-native-action-cable": "1.1.0",
    "@react-native-async-storage/async-storage": "1.16.1",
    "@react-native-clipboard/clipboard": "1.9.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "5.9.10",
    "@react-native-community/push-notification-ios": "1.10.1",
    "@react-native-firebase/app": "12.9.3",
    "@react-native-firebase/messaging": "12.9.3",
    "@react-navigation/bottom-tabs": "5.11.15",
    "@react-navigation/native": "5.9.8",
    "@react-navigation/stack": "5.14.9",
    "@sentry/react-native": "2.6.2",
    "@ui-kitten/components": "5.0.0",
    "@ui-kitten/eva-icons": "5.0.0",
    "@ui-kitten/metro-config": "5.0.0",
    "axios": "0.26.0",
    "i18n-js": "3.8.0",
    "lodash.filter": "4.6.0",
    "lodash.groupby": "4.6.0",
    "markdown-it": "12.3.2",
    "md5": "2.3.0",
    "moment": "2.29.1",
    "posthog-react-native": "1.1.3",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-hook-form": "7.27.1",
    "react-native": "0.64.3",
    "react-native-actions-sheet": "0.6.0",
    "react-native-animatable": "1.3.3",
