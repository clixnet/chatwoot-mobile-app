    "@bam.tech/react-native-make": "3.0.3",
    "@react-native-community/eslint-config": "1.1.0",
    "babel-jest": "25.5.1",
    "babel-plugin-module-resolver": "4.1.0",
    "detox": "17.14.9",
    "eslint": "6.8.0",
    "eslint-plugin-detox": "1.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "4.3.8",
    "jest": "25.5.4",
